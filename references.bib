@inproceedings{saffry2014,
author = {Saffry, Z.A.C. and Majid, Dayang Laila and Romli, Fairuz Izzuddin and Mustapha, Faizal and Abdullah, Ermira Junita},
title = {Identification of Modal Properties of Composite Thin Plate Using OMA in Wind Tunnel Environment},
year = {2014},
month = {1},
volume = {446},
pages = {606--610},
booktitle = {Advanced Research in Material Science and Mechanical Engineering},
series = {Applied Mechanics and Materials},
